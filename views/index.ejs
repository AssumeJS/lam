<% include _header%>

	<h1>Welcome to Lean About Me</h1>

	<% users.forEach(function(user){ %>
		<div class="panel panel-default">
			<div class="panel-heading">
				<a href="/users/<%= users.name() %">
					<% user.name() %>
				</a>
			</div>
			<% if (user.bio) { %>
			<div class="panel-body">
				<%= user.bio %>
			</div>
			<% } %>
		</div>
    <%	})  %>

<% include _footer%>